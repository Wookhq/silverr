{
  "name": "silverr",
  "private": true,
  "version": "0.0.1",
  "description": "Silverr app",
  "author": "Wookhq ORG",
  "main": "electron-main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "electron:prod": "npm run build && electron ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "daisyui": "^5.1.13",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.0.0",
    "vite": "^7.0.4",
    "wait-on": "^9.0.0"
  },
  "build": {
    "appId": "org.wookhq.Silverr",
    "productName": "Silverr",
    "electronVersion": "38.1.2",
    "linux": {
      "target": [
        "flatpak"
      ],
      "category": "Utility",
      "icon": "src/assets/logo.svg"
    }
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "svelte-motion": "^0.12.2",
    "tailwind-merge": "^3.3.1"
  }
}
